{
  "name": "doitright-backend",
  "version": "1.0.0",
  "description": "Devops maturity measuring tool",
  "main": "handler.js",
  "scripts": {
    "analyse": "npm run lint",
    "clean": "rimraf .build .serverless .nyc_output coverage",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "prebuild": "npm run clean && npm run analyse",
    "build": "serverless package",
    "predeploy": "npm run clean && npm run analyse",
    "deploy": "serverless deploy",
    "deploy:prod": "npm run deploy -- --stage production",
    "deploy:qa": "npm run deploy -- --stage qa",
    "deploy:init": "serverless create_domain",
    "lint": "tslint --project . --format verbose",
    "start": "export NODE_ENV=local && serverless offline start",
    "start_test": "DB_ENV=development npm run test src/app.ts",
    "start_beta_test": "DB_ENV=production npm run test src/app.ts",
    "start-win": "set NODE_ENV=local&& serverless offline start",
    "start_test-win": "set DB_ENV=development&& npm run test src/app.ts",
    "start_beta_test-win": "set DB_ENV=production&& npm run test src/app.ts",
    "pretest": "npm run clean",
    "test": "nyc mocha --config ./node_modules/mocha/lib/mocharc.json",
    "test:integration": "mocha --config ./test/integration/.mocharc.yml"
  },
  "keywords": [
    "aws",
    "lambda",
    "serverless",
    "tslint",
    "typescript"
  ],
  "dependencies": {
    "@elastic/elasticsearch": "^7.10.0",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/jwk-to-pem": "^2.0.1",
    "@types/uuid": "^3.4.5",
    "amazon-cognito-identity-js": "^3.0.14",
    "body-parser": "^1.18.3",
    "bunyan": "^1.8.12",
    "class-validator": "^0.9.1",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "husky": "^1.2.0",
    "json2xls": "^0.1.2",
    "jsonwebtoken": "^8.5.1",
    "jwk-to-pem": "^2.0.1",
    "module-alias": "^2.2.0",
    "moment": "^2.23.0",
    "nodemailer": "^4.7.0",
    "otp-generator": "^1.1.0",
    "serverless": "^2.15.0",
    "serverless-http": "^1.8.0",
    "simple-excel-to-json": "^2.2.1",
    "sinon": "^7.1.1",
    "uuid": "^3.3.2",
    "xlsx": "^0.14.1"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.27",
    "@types/bunyan": "^1.8.6",
    "@types/chai": "^4.1.7",
    "@types/chance": "^1.0.4",
    "@types/cors": "^2.8.5",
    "@types/express": "^4.17.0",
    "@types/mocha": "^5.2.7",
    "@types/module-alias": "^2.0.0",
    "@types/node": "^12.12.56",
    "@types/request": "^2.48.1",
    "@types/request-promise-native": "^1.0.16",
    "async": "^3.1.0",
    "aws-sdk": "^2.749.0",
    "chai": "^4.2.0",
    "chance": "^1.0.18",
    "coveralls": "^3.0.4",
    "mocha": "^8.1.3",
    "nyc": "^14.1.1",
    "request-promise-native": "^1.0.7",
    "rimraf": "^2.6.3",
    "serverless-aws-documentation": "^1.1.0",
    "serverless-domain-manager": "^3.2.2",
    "serverless-offline": "^5.12.1",
    "serverless-plugin-typescript": "^1.1.7",
    "serverless-stack-output": "^0.2.3",
    "source-map-support": "^0.5.12",
    "ts-mockito": "^2.3.1",
    "ts-node": "^8.3.0",
    "tslint": "^5.17.0",
    "typescript": "^3.5.2"
  }
}
